# CMake clm configure file

# Now run the build script
set(command sh @CLM_sh_build@)
execute_process(
  COMMAND ${command}
  RESULT_VARIABLE result
  WORKING_DIRECTORY "@CLM_source_dir@/src"
)

if (result)
  set(msg "clm build command failed")
  foreach(arg IN LISTS command )
    set(msg "${msg} '${arg}'")
  endforeach()
  message(FATAL_ERROR ${msg})
endif()  

         
