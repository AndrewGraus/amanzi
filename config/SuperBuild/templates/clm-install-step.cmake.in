# CMake CLM configure file

# Now run the patch script
execute_process(
  COMMAND sh @CLM_sh_install@
  RESULT_VARIABLE result
  WORKING_DIRECTORY "@CLM_source_dir@"
)

if (result)
  set(msg "CLM install command failed")
  foreach(arg IN LISTS command )
    set(msg "${msg} '${arg}'")
  endforeach()
  message(FATAL_ERROR ${msg})
endif()  

                            
