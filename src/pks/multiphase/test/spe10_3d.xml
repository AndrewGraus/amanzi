          <Parameter name="region" type="string" value="All"/>
          <Parameter name="residual saturation wet" type="double" value="0.0"/>
          <Parameter name="residual saturation nonwet" type="double" value="0.0"/>
          <Parameter name="entry pressure" type="double" value="3.0e4"/>
          <Parameter name="brooks corey lambda" type="double" value="2.5"/>
          -->

